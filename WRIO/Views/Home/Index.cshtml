    <style>
        [ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak {
            display: none;
        }
    </style>
</head>

    <body id="@ViewBag.user">

        <div ng-cloak class="container-liquid" ui-view></div>
        <div id="id-loading-page" class="loading-outer">
            <div class="loading-container">
            <h1>Official webRunes node</h1>
            <p>Alpha stage, certain issues and slow connection may be expected</p>
            <div class="loading-inner">
                <img src="/app/img/logo_wrio.png" />
                <div class="loading-progress loading-progress-striped loading-active">
                    <div class="loading-progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
                <p>Loading... please wait</p>
            </div>
        </div>
        </div>
        

        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
        <script src="/app/js/bootstrap.min.js"></script>
        <script src="/app/js/application.js"></script>
        <script src="/app/js/offcanvas.js"></script>
    
        <script src="http://oauth.wr.io:6284/download/latest/oauth.js"></script>

        <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.10/angular.min.js"></script>
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/angular")
        @Scripts.Render("~/bundles/application")
    </body>
</html>

